{
    "table_definition": {
        "$ref": "${table_definition_dir}/cloud_public/production/nsf_divisions.json#/table_definition"
    },
    "source_query": {
        "records": {
            "nsf_directorate_id": "nd.nsf_directorate_id",
            "name": "snv.name",
            "full_name": "snv.full_name"
        },
        "joins": [
            {
                "name": "staging_nsf_divisions",
                "schema": "${SOURCE_SCHEMA}",
                "alias": "snv"
            },
            {
                "name": "nsf_directorates",
                "schema": "${SOURCE_SCHEMA}",
                "alias": "nd",
                "on": "snv.nsf_directorate = nd.name"
            }
        ]
    }
}
