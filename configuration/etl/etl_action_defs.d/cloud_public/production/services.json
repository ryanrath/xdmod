{
    "table_definition": {
        "$ref": "${table_definition_dir}/cloud_public/production/services.json#/table_definition"
    },
    "source_query": {
        "records": {
            "name": "DISTINCT ss.service",
            "full_name": "CONCAT(pc.display, ' - ', ss.service)",
            "cloud_provider_id": "pc.cloud_provider_id"
        },
        "joins": [
            {
                "name": "staging_spend",
                "schema": "${SOURCE_SCHEMA}",
                "alias": "ss"
            },
            {
                "name": "cloud_providers",
                "schema": "${SOURCE_SCHEMA}",
                "alias": "pc",
                "on": "pc.name = ss.cloud_provider"
            }
        ]
    }
}
