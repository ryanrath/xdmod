{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "authentication_error",
    "type": "object",
    "properties": {
        "success": {
            "enum": [
                false
            ]
        },
        "count": {
            "type": "integer"
        },
        "total": {
            "type": "integer"
        },
        "totalCount": {
            "type": "integer"
        },
        "results": {
            "type": "array"
        },
        "data": {
            "type": "array"
        },
        "message": {
            "const": "An error was encountered while attempting to process the requested authorization procedure."
        },
        "code": {
            "type": "integer"
        }
    }
}
